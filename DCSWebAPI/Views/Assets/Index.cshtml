@model IEnumerable<DCSWebAPI.Models.Assets>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New Asset", "Create")
</p>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.asset_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.asset_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.asset_price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.installed_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.class_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.type_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.brand_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.model_id)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.asset_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.asset_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.asset_price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.installed_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.class_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.type_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.brand_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.model_id)
        </td>
    
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.asset_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.asset_id })
        </td>
    </tr>
}

</table>
